<%- include('partials/header'); -%>
<h1>Compose</h1>
<form class="form-group" action="/compose" method="post">

    <div class="form-group">
        <label>Title</label>
        <input type="text" name="postTitle" class="form-control" id="exampleFormControlInput1">
    </div>

    <div class="form-group">
        <label>Post .</label>
        <textarea name="postBody" class="form-control" id="exampleFormControlTextarea1" rows="3"></textarea>
    </div>
    <button type="submit" class="btn btn-primary" name="submit">Publish</button>
</form>
<%- include('partials/footer') ; -%>